{
  "expo": {
    "name": "Cyklofit",
    "slug": "Cyklofit",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icons/logo-small.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo-tag.png",
      "resizeMode": "contain",
      "backgroundColor": "#171725"
    
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo-small.png",
        "backgroundColor": "#171725"
       
      },
      "package": "com.cyklofit"
    },
    "web": {
      "favicon": "./assets/icons/logo-tag.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b8f472a1-0052-4f83-aede-724cf3f69dd6"
      }
    }
  }
}
